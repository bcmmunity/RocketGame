@{
	Layout = null;
}


<table>
	<thead>
		<tr>
			<th>Такт @ViewBag.number</th>
		</tr>
	</thead>
	<tbody>
		@for (int i = 0; i < ViewBag.moves.Length; i++)
		{
		<tr>
			<th>
				@ViewBag.moves[i]
			</th>
		</tr>
		}
	</tbody>
</table>


<style>
	.tick {
		width: 250px;
		background-color: Highlight;
		border-radius: 50px;
		color: white;
		float: left;
	}
</style>
